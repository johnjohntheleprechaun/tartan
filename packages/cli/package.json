{
    "name": "@tartan/cli",
    "version": "0.1.2",
    "description": "CLI for the Tartan static site generator",
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rm -r dist && npx tsc && chmod +x dist/index.js"
    },
    "bin": {
        "tartan": "dist/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/johnjohntheleprechaun/tartan.git"
    },
    "author": "johnjohntheleprechaun",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/johnjohntheleprechaun/tartan/issues"
    },
    "homepage": "https://github.com/johnjohntheleprechaun/tartan#readme",
    "dependencies": {
        "@tartan/core": "^0.2.1",
        "commander": "^13.1.0"
    },
    "devDependencies": {
        "@commander-js/extra-typings": "^13.1.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.2"
    }
}
